let Window = inherit {} : {
  local width;
  local height;
  local pos_x;
  local pos_y;
  constructor = method(width, height) {
    this.width = width;
    this.height = height;
  };
  moveto = method(posx, posy) {
    this.pos_x = posx;
    this.pos_y = posy;
  };
}

